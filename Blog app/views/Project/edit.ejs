<%- include ("../partials/header") %>

<div class="container">
	<div class="row">
		<h1 style="text-align: center;">Edit Project</h1>
		<div style="width: 70%; margin: 25px auto;">
			<form action="/projects/<%= project._id %>?_method=PUT" method="POST">
				<div class="form-group">
					<label>Title</label>
					<input class="form-control" type="text" name="project[title]" value="<%= project.title %>">
				</div>
				<div class="form-group">
					<label>Team Members</label>
					<input class="form-control" type="text" name="project[teamMembers]" value="<%= project.teamMembers %>">
				</div>
				<div class="form-group">
					<label>Department</label>
					<input class="form-control" type="text" name="project[branch]" value="<%= project.branch %>">
				</div>
				<div class="form-group">
					<label>Display Image</label>
					<input class="form-control" type="text" name="project[image1]" value="<%= project.image1 %>">
                </div>
                <div class="form-group">
					<label>Image 2</label>
					<input class="form-control" type="text" name="project[image2]" value="<%= project.image2 %>">
                </div>
                <div class="form-group">
					<label>Image 3</label>
					<input class="form-control" type="text" name="project[image3]" value="<%= project.image3 %>">
                </div>
                <div class="form-group">
					<label>Image 4</label>
					<input class="form-control" type="text" name="project[image4]" value="<%= project.image4 %>">
                </div>
                <div class="form-group">
					<label>Image 5</label>
					<input class="form-control" type="text" name="project[image5]" value="<%= project.image5 %>">
                </div>
                <div class="form-group">
					<label>Image 6</label>
					<input class="form-control" type="text" name="project[image6]" value="<%= project.image6 %>">
                </div>
                <div class="form-group">
					<label>Image 7</label>
					<input class="form-control" type="text" name="project[image7]" value="<%= project.image7 %>">
                </div>
                <div class="form-group">
					<label>Image 8</label>
					<input class="form-control" type="text" name="project[image8]" value="<%= project.image8 %>">
				</div><div class="form-group">
					<label>Image 9</label>
					<input class="form-control" type="text" name="project[image9]" value="<%= project.image9 %>">
                </div>
                <div class="form-group">
					<label>Image 10</label>
					<input class="form-control" type="text" name="project[image10]" value="<%= project.image10 %>">
                </div>
				<div class="field">
                    <label>Event Content</label>
                    <textarea class="form-control" rows="10" name="project[description]"><%= project.description %></textarea>
                </div>
				<div class="form-group" style="padding-top: 10px;">
					<button class="btn btn-lg btn-primary btn-block">Submit</button>
				</div>
				</form>
			<a href="/projects">Go back</a>
		</div>
		
	</div>
</div>


<%- include ("../partials/footer") %>